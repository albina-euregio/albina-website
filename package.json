{
  "name": "albina-project",
  "version": "0.0.2",
  "description": "Albina project webpage",
  "main": "app/main.jsx",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "html-to-react": "^1.3.3",
    "react": "15.x",
    "react-dom": "15.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "7.x",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.x",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "^1.3.22",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "fbjs": "^0.8.16",
    "file-loader": "^1.1.11",
    "htmltojsx": "^0.3.0",
    "husky": "^0.12.0",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "open-browser-webpack-plugin": "0.0.3",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "sockjs-client": "^1.1.4",
    "style-loader": "~0.13.1",
    "validate-commit-msg": "2.x",
    "webpack": "^2.7.0",
    "webpack-dev-server": "1.x"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
