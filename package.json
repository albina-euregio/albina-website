{
  "name": "albina-website",
  "description": "Avalanche. report - Avalanche Warning Services Tirol - South Tyrol - Trentino",
  "homepage": "https://avalanche.report/",
  "version": "0.2.0",
  "private": false,
  "main": "app/main.jsx",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "start-dev": "webpack-dev-server --mode development --env dev",
    "test": "npm run eslint",
    "eslint": "eslint --ext=js,jsx .",
    "build": "npm run clean-dist && webpack --mode production --env prod",
    "build-beta": "npm run clean-dist && webpack --mode production --env beta",
    "build-dev": "npm run clean-dist && webpack --mode production --env dev",
    "deploy": "rsync -avz -e 'ssh -p 2201' --delete --progress dist/ karto@80.120.70.88:/var/www/html/albina-web/",
    "deploy-beta": "rsync -avz -e 'ssh -p 2201' --delete --progress dist/ karto@80.120.70.88:/var/www/html/albina-web-beta/",
    "deploy-dev": "rsync -avz -e 'ssh -p 2201' --delete --progress dist/ karto@80.120.70.88:/var/www/html/albina-web-dev/",
    "clean-dist": "rm -f -r -d dist"
  },
  "dependencies": {
    "@loadable/component": "^5.13.1",
    "@sentry/browser": "^5.23.0",
    "animejs": "^2.2.0",
    "axios": "^0.19.2",
    "fluidvids.js": "github:toddmotto/fluidvids",
    "format-message": "^6.2.3",
    "html-to-react": "^1.4.3",
    "jquery": "^3.5.1",
    "leaflet": "^1.7.1",
    "leaflet-geonames": "^0.4.9",
    "leaflet-gesture-handling": "^1.1.8",
    "leaflet.locatecontrol": "^0.63.0",
    "leaflet.markercluster": "^1.4.1",
    "leaflet.sync": "^0.2.4",
    "magnific-popup": "^1.1.0",
    "mobx": "^4.15.6",
    "mobx-react": "^5.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-iframe": "^1.8.0",
    "react-intl": "^3.12.1",
    "react-leaflet": "^1.9.1",
    "react-leaflet-control": "^1.4.1",
    "react-meta-tags": "^0.7.4",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-router-scroll-4": "^1.0.0-beta.2",
