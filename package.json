{
  "name": "albina-website",
  "description": "Avalanche. report - Avalanche Warning Services Tirol - South Tyrol - Trentino",
  "homepage": "https://avalanche.report/",
  "version": "0.2.0",
  "private": false,
  "main": "app/main.jsx",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "test": "npm run eslint",
    "eslint": "eslint --ext=js,jsx .",
    "build": "npm run clean-dist && webpack --mode production --env prod",
    "build-beta": "npm run clean-dist && webpack --mode production --env beta",
    "build-dev": "npm run clean-dist && webpack --mode production --env dev",
    "deploy": "rsync -avz -e 'ssh -p 2201' --progress --exclude=config.json dist/ karto@80.120.70.88:/var/www/html/albina-web/",
    "deploy-beta": "rsync -avz -e 'ssh -p 2201' --delete --progress --exclude=config.json --exclude=.htaccess dist/ karto@80.120.70.88:/var/www/html/albina-web-beta/",
    "deploy-dev": "rsync -avz -e 'ssh -p 2201' --delete --progress --exclude=config.json --exclude=.htaccess dist/ karto@80.120.70.88:/var/www/html/albina-web-dev/",
    "clean-dist": "rm -f -r -d dist"
  },
  "dependencies": {
    "@loadable/component": "^5.10.2",
    "@sentry/browser": "^5.9.0",
    "animejs": "^2.2.0",
    "axios": "^0.19.0",
    "fluidvids.js": "github:toddmotto/fluidvids",
    "format-message": "^6.2.3",
    "html-to-react": "^1.3.4",
    "jquery": "^3.4.1",
    "leaflet": "^1.5.1",
    "leaflet-geonames": "^0.4.7",
    "leaflet-gesture-handling": "^1.1.8",
    "leaflet.locatecontrol": "^0.63.0",
    "leaflet.markercluster": "^1.4.1",
    "leaflet.sync": "^0.2.4",
    "mobx": "^4.11.0",
    "mobx-react": "^5.4.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-iframe": "^1.8.0",
    "react-intl": "^2.9.0",
    "react-leaflet": "^1.9.1",
    "react-leaflet-control": "^1.4.1",
    "react-meta-tags": "^0.7.4",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-router-scroll-4": "^1.0.0-beta.2",
    "regenerator-runtime": "^0.13.2",
    "url-search-params-polyfill": "^5.1.0",
    "window.requestanimationframe": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "imagemin-webp-webpack-plugin": "^3.3.0",
    "intl": "^1.2.5",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
