{
  "name": "albina-website",
  "description": "Avalanche. report - Avalanche Warning Services Tirol - South Tyrol - Trentino",
  "homepage": "https://avalanche.report/",
  "version": "0.2.0",
  "type": "module",
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "start": "vite",
    "start-beta": "vite --base=/beta/",
    "start-dev": "vite --base=/dev/",
    "test": "eslint --max-warnings 0 --ignore-pattern app/components/weather/",
    "build": "vite build",
    "build-prod": "vite build",
    "build-beta": "vite build --base=/beta/",
    "build-dev": "vite build --base=/dev/",
    "changelog": "git-cliff --unreleased --prepend CHANGELOG.md --tag",
    "changelog-latest": "git-cliff --latest --prepend CHANGELOG.md"
  },
  "dependencies": {
    "@eaws/micro-regions_elevation_properties": "^7.0.5",
    "@eaws/micro-regions_properties": "^7.0.5",
    "@eaws/outline_properties": "^7.0.5",
    "@floating-ui/react": "^0.26.23",
    "@floating-ui/react-dom-interactions": "^0.13.3",
    "@mapbox/point-geometry": "^1.1.0",
    "@nanostores/react": "^0.8.0",
    "@sentry/react": "^8.7.0",
    "diff-match-patch": "^1.0.5",
    "htmr": "^1.0.2",
    "husky": "^9.0.11",
    "jquery": "^3.7.1",
    "leaflet": "^1.9.4",
    "leaflet-geonames": "^0.4.9",
    "leaflet-gesture-handling": "^1.2.2",
    "leaflet.locatecontrol": "^0.79.0",
    "leaflet.markercluster": "^1.5.3",
    "leaflet.sync": "^0.2.4",
    "leaflet.vectorgrid": "^1.3.0",
    "nanostores": "^0.11.3",
    "normalize.css": "^8.0.1",
    "pmtiles": "^4.2.1",
    "purecss-sass": "3.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-easy-swipe": "0.0.23",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.0.2",
    "react-string-replace": "^1.1.1",
    "uplot": "^1.6.31",
    "uplot-react": "^1.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.44.1",
    "@types/diff-match-patch": "^1.0.36",
    "@types/leaflet": "^1.9.15",
    "@types/leaflet.vectorgrid": "^1.3.5",
    "@types/react": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "5.1.0-rc-91061073-20241121",
    "git-cliff": "^2.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "3.3.1",
    "sass": "^1.83.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.2"
  },
  "keywords": [],
  "author": "",
  "repository": {
    "url": "https://gitlab.com/albina-euregio/albina-website"
