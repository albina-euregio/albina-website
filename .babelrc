{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-proposal-optional-chaining"],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": ["last 3 versions", "ie >= 9"]
        },
        "useBuiltIns": "entry",
        "corejs": "2"
      }
    ],
    "@babel/react"
  ]
}
